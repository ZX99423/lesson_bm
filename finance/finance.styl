@font-face{
  font-family:'icomoon';
  src:url("./fonts/icomoon.eot?yisc3l");
  src:url("./fonts/icomoon.eot?yisc3l#iefix") format("embedded-opentype"),
  url("./fonts/icomoon.woff2?yisc3l") format("woff2"),
  url("./fonts/icomoon.ttf?yisc3l") format("truetype"),
  url("./fonts/icomoon.woff?yisc3l") format("woff"),
  url("./fonts/icomoon.svg?yisc3l#icomoon") format("svg");
}

*
  margin 0
  padding 0

a
  text-decoration none
  color black

.sleft
  float left
  border-right 1px solid #BDDAF5
  width 182px
  margin 6px 0 5px 12px 
.sright
  float left
  margin 6px 0 5px 12px 

.finance-wrapper
  height 102px
  width 400px
  border 1px solid #CAE0F3
  border-top 3px solid #9Ec9ee
  overflow hidden

.st-item .detail .price 
    display inline-block

.st-item .detail span
  color #DF4D42

.st-item .detail-down span
    color #2DAA50;

[class^="icon-num-"], [class*=" icon-num-"]
    font-family 'icomoon' !important

.icon-num-2:before
  content "\32"
.icon-num-3:before
    content: "\33"
.icon-num-0:before
    content: "\30"
.icon-num-7:before
    content: "\37"
.icon-num-5:before
    content: "\35"
.icon-num-d:before
    content: "\2e"
.icon-num-8:before
    content: "\38"